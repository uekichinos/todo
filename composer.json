{
  "name": "khyrie/todo",
  "description": "a todo package for laravel",
  "license": "MIT",
  "keywords": [
    "todo"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Ueki Chinos",
      "email": "megatzulkhairi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "khyrie\\Todo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "khyrie\\Todo\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "khyrie\\Todo\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
